{
  "name": "stripes-maker",
  "version": "1.0.0",
  "description": "Node.js stripe pattern generator and maker. It can create multi-line static and animated SVG stripe patterns.",
  "type": "module",
  "scripts": {
    "stripes-maker": "node ./bin/cli.js -- create test.svg -w 300 -h 300 -bgc '#fff' -lc '#6573ab' -lw 35 -ld 35 -lr 45",
    "start": "ts-node-esm ./src/index.ts"
  },
  "bin": {
    "stripes-maker": "./bin/cli.js"
  },
  "keywords": [
    "stripes",
    "pattern",
    "patterns",
    "svg",
    "svg animations",
    "knitting",
    "knitting pattern",
    "crochet",
    "crochet pattern",
    "animated svg"
  ],
  "author": "Miriam Zusin <miriam.zusin@gmail.com>",
  "license": "SEE LICENSE IN LICENSE.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/mzusin/stripes-maker.git"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "chalk": "^5.2.0",
    "commander": "^10.0.0",
    "fs-extra": "^11.1.1",
    "jsdom": "^21.1.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.3"
  }
}
