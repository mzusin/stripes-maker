{
  "name": "stripes-maker",
  "version": "1.0.1",
  "description": "Node.js stripe pattern generator and maker. It can create multi-line static and animated SVG stripe patterns.",
  "type": "module",
  "scripts": {
    "start": "ts-node-esm ./src/index.ts stripes.config.js",
    "make": "node ./bin/cli.js stripes.config.js",
    "website:build": "node ./run/website.js",
    "website:watch": "node ./run/website.js -- watch",
    "website:preview": "http-server ./website"
  },
  "bin": {
    "stripes-maker": "./bin/cli.js"
  },
  "keywords": [
    "stripes",
    "pattern",
    "patterns",
    "svg",
    "svg animations",
    "knitting",
    "knitting pattern",
    "crochet",
    "crochet pattern",
    "animated svg"
  ],
  "author": "Miriam Zusin <miriam.zusin@gmail.com>",
  "license": "SEE LICENSE IN LICENSE.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/mzusin/stripes-maker.git"
  },
  "devDependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "@types/node": "^18.15.11",
    "autoprefixer": "^10.4.14",
    "esbuild": "^0.17.16",
    "fs-extra": "^11.1.1",
    "highlight.js": "^11.7.0",
    "http-server": "^14.1.1",
    "js-beautify": "^1.14.7",
    "jsdom": "^21.1.1",
    "postcss": "^8.4.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "tailwindcss": "^3.3.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.3",
    "uglifycss": "^0.0.29"
  },
  "dependencies": {
    "mz-math": "^3.0.4",
    "mz-svg": "^1.0.9",
    "toolcool-color-picker": "^1.0.14"
  }
}
